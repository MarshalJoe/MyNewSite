{% block contact %}

{% setcontent write = 'page/writing' %}

<div class="pencil-pic" style="background: url({{ write.image|image }}); background-size: cover" data-stellar-background-ratio="0.5">
</div>

<div id="contactAnchor" class="contact-section">
	<div id="contact-form">
			<div class="container">
				<br>
				<form method='post' action=''>
					<label for="email">Email*</label>
	    		<input id="email" name='email' type='text' required>
	    		<label for="subject">Subject</label>
	    		<input id="subject" name='subject' type='text'>
	    		<label for="message">Message*</label>
	    		<textarea id="message" name='message' placeholder="How 'bout that Game of Thrones, eh?" required></textarea>
       		<input class="radius button" type='submit' value="Send">
  			</form>
  			<div id="loading">
    			Sending your message...
  			</div>
  			<div id="success">
  				Message sent.
  			</div>

			</div>
		
		</div>

</div>


{% endblock %}





